{
  "compilerOptions": {
    "target": "ES2020", // You can change this to your desired ECMAScript version
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist" // Adjust the output directory as needed
  },
  "include": ["src/**/*"], // Adjust this to point to your TypeScript files
  "exclude": ["node_modules", "**/*.spec.ts"]
}

